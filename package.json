    "d3-selection": "^1.3.2",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "^2.10.0",
    "node-schedule": "^1.3.0",
    "opened-captions": "^1.0.4",
